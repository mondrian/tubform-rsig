<h1>Listando motivos_assistencia</h1>

<table>
  <tr>
    <th>Descricao</th>
  </tr>

<% @motivos_assistencia.each do |motivo_assistencia| %>
  <tr>
    <td><%=h motivo_assistencia.descricao %></td>
    <td><%= link_to 'Listar', motivo_assistencia %></td>
    <td><%= link_to 'Editar', edit_motivo_assistencia_path(motivo_assistencia) %></td>
    <td><%= link_to 'Excluir', motivo_assistencia, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Novo motivo_assistencia', new_motivo_assistencia_path %>
